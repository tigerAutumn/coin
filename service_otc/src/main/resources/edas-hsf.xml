	</hsf:provider>
	
	<hsf:provider id="otcUserServiceProvider" interface="com.qkwl.common.rpc.otc.IOtcUserService"
				  ref="otcUserService" version="1.1" group="otc">
	</hsf:provider>
	
	<hsf:provider id="otcOrderServiceProvider" interface="com.qkwl.common.rpc.otc.OtcOrderService"
				  ref="otcOrderService" version="1.1" group="otc">
